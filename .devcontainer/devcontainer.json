{
  "name": "financials",
  "build": {
    "dockerfile": "../.gitpod.Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "remoteUser": "gitpod",
  "containerUser": "gitpod",
  "workspaceFolder": "/workspace/financials",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/financials,type=bind",
  "forwardPorts": [
    8501
  ],
  "portsAttributes": {
    "8501": {
      "label": "Port 8501",
      "onAutoForward": "openPreview",
      "elevateIfNeeded": true
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "ms-toolsai.vscode-jupyter-slideshow",
        "streamlit.streamlit"
      ]
    }
  }
}